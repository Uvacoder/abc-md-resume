{
  "compilerOptions": {
    "target": "ES6",
    "jsx": "react-jsx",
    "useDefineForClassFields": true,
    "module": "ES6",
    "lib": [
      "ES6",
      "DOM",
    ],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "include": [
    "./src"
  ]
}